{

  "manifest_version": 2,
  "name": "Borderify",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "chrome_url_overrides" : {
    "newtab": "new-tab.html"
  },

  "content_scripts": [
    {
      "matches": ["*://*.google.com/*"],
      "js": ["demo.js"]
    }
  ]



}